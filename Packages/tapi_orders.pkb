    PROCEDURE web_del (
                      p_order_id IN orders.order_id%TYPE,
                      p_hash IN varchar2
                      )
   IS
      l_scope logger_logs.scope%TYPE := gc_scope_prefix || 'web_del';
      l_params logger.tab_param;
      l_orders_rec orders_rt;
   BEGIN

      logger.append_param(l_params, 'p_order_id', web_del.p_order_id);
      logger.LOG('START', l_scope, NULL, l_params);
      logger.LOG('Deleting record', l_scope);

      OPEN orders_cur(
                            web_del.p_order_id
                            );

      FETCH orders_cur INTO l_orders_rec;

      IF orders_cur%NOTFOUND THEN
         CLOSE orders_cur;
         RAISE e_row_missing;
      ELSE
         IF web_del.p_hash != l_orders_rec.hash THEN
            CLOSE orders_cur;
            RAISE e_ol_check_failed;
         ELSE
            DELETE FROM orders
            WHERE CURRENT OF orders_cur;

            CLOSE orders_cur;
         END IF;
      END IF;


      logger.LOG('END', l_scope);

   EXCEPTION
     WHEN e_ol_check_failed
     THEN
        raise_application_error (-20000 , 'Current version of data in database has changed since last page refresh.');
     WHEN e_row_missing
     THEN
        raise_application_error (-20000 , 'Delete operation failed because the row is no longer in the database.');
     WHEN OTHERS
     THEN
        logger.log_error('Unhandled Exception', l_scope, NULL, l_params);
        RAISE;
   END web_del;

