                       comments = p_orders_rec.comments,
                       request_time = p_orders_rec.request_time,
                       response_time = p_orders_rec.response_time,
                       arrival_time = p_orders_rec.arrival_time,
                       state_id = p_orders_rec.state_id
               WHERE CURRENT OF orders_cur;
            END IF;

            CLOSE orders_cur;
         END IF;
      END IF;

      logger.LOG('END', l_scope);

   EXCEPTION
     WHEN e_ol_check_failed
     THEN
        raise_application_error (-20000 , 'Current version of data in database has changed since last page refresh.');
     WHEN e_row_missing
     THEN
        raise_application_error (-20000 , 'Update operation failed because the row is no longer in the database.');
     WHEN OTHERS
     THEN
        logger.log_error('Unhandled Exception', l_scope, NULL, l_params);
        RAISE;
   END web_upd;

