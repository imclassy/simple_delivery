       logger.LOG('Updating table', l_scope);

      OPEN orders_cur(
                             web_upd.p_orders_rec.order_id
                        );

      FETCH orders_cur INTO l_orders_rec;

      IF orders_cur%NOTFOUND THEN
         CLOSE orders_cur;
         RAISE e_row_missing;
      ELSE
         IF p_orders_rec.hash != l_orders_rec.hash THEN
            CLOSE orders_cur;
            RAISE e_ol_check_failed;
         ELSE
            IF NVL(p_ignore_nulls, FALSE)
