                       arrival_time = NVL(p_orders_rec.arrival_time,arrival_time),
                       state_id = NVL(p_orders_rec.state_id,state_id)
               WHERE CURRENT OF orders_cur;
            ELSE
                UPDATE   orders
                   SET order_id = p_orders_rec.order_id,
                       user_id = p_orders_rec.user_id,
                       shop_id = p_orders_rec.shop_id,
                       geoloc = p_orders_rec.geoloc,
